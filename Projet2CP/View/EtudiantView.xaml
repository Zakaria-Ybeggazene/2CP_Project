        <Label Content="Informations" FontFamily="Montserrat" FontSize="26" FontWeight="SemiBold" Height="Auto" Grid.Column="1" HorizontalAlignment="Left" Margin="58,21,0,0" Name="Label4" VerticalAlignment="Top" Width="192" >
