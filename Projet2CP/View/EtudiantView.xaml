        <Label Content="Nom Mere" FontFamily="Montserrat" FontSize="15" FontWeight="Light" Foreground="#FF123456" Height="29" HorizontalAlignment="Left" Margin="13,222,0,0" Name="NomMereHint" VerticalAlignment="Top" VerticalContentAlignment="Center" Width="Auto" Grid.Row="1" Grid.Column="1" />
