                                    <DataGridTextColumn Header="Moy-Juin" Width="80"  FontFamily="Montserrat" Binding="{Binding Value.Noju}" />
                                    <DataGridTextColumn Header="Moy-Septembre" Width="100" FontFamily="Montserrat" Binding="{Binding Value.Nora}" />
                                    <DataGridTextColumn Header="Note Synthese" Width="100"  FontFamily="Montserrat" Binding="{Binding Value.Nosy}"/>
