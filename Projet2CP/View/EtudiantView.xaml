        <Label Content="Prenom " FontFamily="Montserrat" FontSize="15" FontWeight="Light" Foreground="#FF123456" Height="29" HorizontalAlignment="Left" Margin="864,179,0,0" Name="PrenomHint" VerticalAlignment="Top" VerticalContentAlignment="Bottom" Width="70" />
        <Label Content="اللقب" FontFamily="Montserrat" FontSize="15" FontWeight="Light" Foreground="#FF123456" Height="29" HorizontalAlignment="Right" Name="NomHintA" VerticalAlignment="Top" VerticalContentAlignment="Top" Width="43" Margin="0,79,15,0" HorizontalContentAlignment="Right" />
