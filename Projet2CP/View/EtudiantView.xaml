                                </DataGrid.Columns>
                                <DataGrid.RowStyle>
                                    <Style TargetType="DataGridRow">
                                        <Setter Property="Height" Value="30" />
                                    </Style>
                                </DataGrid.RowStyle>
                                <DataGrid.Resources>
                                    <Style TargetType="DataGridColumnHeader">
                                        <Setter Property="Foreground" Value="#123456" />
                                        <Setter Property="FontWeight" Value="SemiBold" />
                                    </Style>
                                </DataGrid.Resources>
                            </DataGrid>
