            <TabControl ItemsSource="{Binding Etudiant.Parcours}" Height="412" HorizontalAlignment="Left" Margin="11,20,0,0" Name="TabControl1" VerticalAlignment="Top" Width="420" Grid.Row="1" BorderBrush="Black">
            <TabControl.Template>
                <ControlTemplate TargetType="{x:Type TabControl}">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <ScrollViewer    HorizontalAlignment="Stretch" Margin="10" VerticalAlignment="Stretch" HorizontalScrollBarVisibility="Visible" VerticalScrollBarVisibility="Hidden" BorderThickness="1" BorderBrush="Black">
                            <TabPanel x:Name="HeaderPanel" IsItemsHost="True" Margin="0,4,0,0"/>
                        </ScrollViewer>
                        <ContentPresenter x:Name="PART_SelectedContentHost" Margin="4" ContentSource="SelectedContent" Grid.Row="1"/>
                    </Grid>
                </ControlTemplate>
            </TabControl.Template>
            <TabControl.ItemTemplate >
                <DataTemplate >
                    
                        <DockPanel Width="60" Height="27">
                        <TextBlock  Text="{Binding Niveau}"  Foreground="#FF123456" FontFamily="Montserrat" VerticalAlignment="Center"  />
                    </DockPanel>
                    
                </DataTemplate>
            </TabControl.ItemTemplate>
            <TabControl.ContentTemplate>
                <DataTemplate>
                    <Grid    Width="Auto" HorizontalAlignment="Left" VerticalAlignment="Bottom" Height="263">
                        <Label Content="Année :" Height="Auto" HorizontalAlignment="Left" Margin="40,-88,0,0" Name="Label5" VerticalAlignment="Top" Width="62" Foreground="#FF123456" FontSize="14" />
                        <Label Content="{Binding Annee}" Height="Auto" HorizontalAlignment="Left" Margin="100,-88,292,0" Name="Label6" VerticalAlignment="Top" Width="61" Foreground="#FF1165B8" FontSize="14" />
                        <Label Content="Séction:" Foreground="#FF123456" Height="Auto" HorizontalAlignment="Left" Margin="40,-64,0,0" Name="Label7" VerticalAlignment="Top" Width="68" FontSize="14" />
                        <Label Content="{Binding Section}" Foreground="#FF1165B8" Height="Auto" HorizontalAlignment="Left" Margin="108,-64,0,0" Name="Label8" VerticalAlignment="Top" Width="61" FontSize="14" />
                        <Label Content="N° groupe:" Foreground="#FF123456" Height="45" HorizontalAlignment="Left" Margin="40,-40,0,284" Name="Label9" Width="86" FontSize="14" VerticalAlignment="Top" />
                        <Label Content="{Binding Groupe}" Foreground="#FF1165B8" Height="Auto" HorizontalAlignment="Left" Margin="121,-40,0,0" Name="Label10" VerticalAlignment="Top" Width="61" FontSize="14" />
                        <Label Content="Moyenne:" FontSize="14" Foreground="#FF123456" Height="Auto" HorizontalAlignment="Right" Margin="0,-91,131,0" Name="Label11" VerticalAlignment="Top" Width="78" />
                        <Label Content="{Binding MoyenneJ}" FontSize="14" Foreground="#FF1165B8" Height="Auto" HorizontalAlignment="Right" Margin="0,-91,68,0" Name="Label12" VerticalAlignment="Top" Width="61" />
                        <Label Content="Décision:" FontSize="14" Foreground="#FF123456" Height="Auto" HorizontalAlignment="Right" Margin="0,-64,136,0" Name="Label13" VerticalAlignment="Top" Width="73" />
                        <Label Content="{Binding Decision}" FontSize="14" Foreground="#FF1165B8" Height="Auto" HorizontalAlignment="Right" Margin="0,-64,74,0" Name="Label14" VerticalAlignment="Top" Width="61" BorderBrush="#FFD18383" />
                        <Label Content="Rang:" FontSize="14" Foreground="#FF123456" Height="65" HorizontalAlignment="Right" Margin="0,-40,157,284" Name="Label15" Width="52" VerticalAlignment="Top" />
                        <Label Content="{Binding Rang}" FontSize="14" Foreground="#FF1165B8" Height="Auto" HorizontalAlignment="Right" Margin="0,-40,78,0" Name="Label16" VerticalAlignment="Top" Width="61" />
                        <Border CornerRadius=" 15" BorderThickness="3" Margin="-1,2,0,0">
                            <Border.Effect>
                                <DropShadowEffect Color="#123456" Opacity="0.3" ShadowDepth="3" RenderingBias="Quality" />
                            </Border.Effect>
                            <DataGrid CanUserResizeColumns="False"  ItemsSource="{Binding Notes}" BorderThickness=" 0" Height="262" Width="400" Style="{StaticResource Mystyle}" Name="Note" AutoGenerateColumns="False" ScrollViewer.CanContentScroll="True" ScrollViewer.VerticalScrollBarVisibility="Auto" ScrollViewer.HorizontalScrollBarVisibility="Auto" GridLinesVisibility="None"  FontFamily="Montserrat" FontSize="15"  IsReadOnly="True" Background="{x:Null}" BorderBrush="{x:Null}">
                                <DataGrid.Columns>
                                    <DataGridTextColumn Header="Module" Width="*" FontFamily="Montserrat" Binding="{Binding Key.CodMat}"  />
                                    <DataGridTextColumn Header="Moy-Juin" Width="65"  FontFamily="Montserrat" Binding="{Binding Value.Noju}" />
                                    <DataGridTextColumn Header="Moy-Septembre" Width="108" FontFamily="Montserrat" Binding="{Binding Value.Nora}" />
                                    <DataGridTextColumn Header="Note Synthese" Width="97"  FontFamily="Montserrat" Binding="{Binding Value.Nosy}"/>
                                    <DataGridTextColumn Header="Eliminatoire" Width="50"  FontFamily="Montserrat" Binding="{Binding Value.Eliminatoire}"/>

                                </DataGrid.Columns>
                                <DataGrid.RowStyle>
                                    <Style TargetType="DataGridRow">
                                        <Setter Property="Height" Value="30" />
                                        <Setter Property="FontSize"  Value="13" />
                                    </Style>
                                </DataGrid.RowStyle>
                                <DataGrid.Resources>
                                    <Style TargetType="DataGridColumnHeader">
                                        <Setter Property="Foreground" Value="#123456" />
                                        <Setter Property="FontWeight" Value="Regular"  />
                                        <Setter Property="FontSize" Value="12"  />
                                    </Style>
                                </DataGrid.Resources>
                            </DataGrid>
                            <Border.BorderBrush>
                                <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
                                    <GradientStop Color="#FF123456" Offset="0" />
                                    <GradientStop Color="#FF1168BF" Offset="1" />
                                </LinearGradientBrush>
                            </Border.BorderBrush>
                        </Border>
                     
                    </Grid>
                </DataTemplate>
            </TabControl.ContentTemplate>
        </TabControl>

        <Label Content="Historique " Height="Auto" HorizontalAlignment="Left" Name="Label1" VerticalAlignment="Top" Width="172" FontSize="26" FontFamily="Montserrat" FontWeight="SemiBold" Margin="14,21,0,0">
