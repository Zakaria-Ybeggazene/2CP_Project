                        <Label Content="{Binding Rang}" FontSize="14" Foreground="#FF1165B8" Height="24" HorizontalAlignment="Right" Margin="0,-50,78,0" Name="Label16" VerticalAlignment="Top" Width="61" />
                    </Grid>
                </DataTemplate>
            </TabControl.ContentTemplate>
