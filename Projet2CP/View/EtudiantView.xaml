        <Label Content="Wilaya" FontFamily="Montserrat" FontSize="15" FontWeight="Light" Foreground="#FF123456" Height="29" HorizontalAlignment="Left" Margin="864,339,0,0" Name="WilayaHint" VerticalAlignment="Top" VerticalContentAlignment="Bottom" Width="63" Background="{x:Null}" HorizontalContentAlignment="Left" />
